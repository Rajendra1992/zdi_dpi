  "common_new_relic_endpoints": {
    "applications": {
      "url": "https://api.newrelic.com/v2/applications.json",
      "description": "List all applications",
      "headers": {
        "X-Api-Key": "YOUR_API_KEY"
      }
    },
    "application_metrics": {
      "url": "https://api.newrelic.com/v2/applications/{application_id}/metrics/data.json",
      "description": "Get metrics for specific application",
      "parameters": {
        "names[]": "Apdex,HttpDispatcher,Errors/all",
        "from": "2024-01-15T09:00:00Z",
        "to": "2024-01-15T10:00:00Z",
        "summarize": "true"
      }
    },
    "insights_query": {
      "url": "https://insights-api.newrelic.com/v1/accounts/{account_id}/query",
      "description": "NRQL query for custom data",
      "parameters": {
        "nrql": "SELECT * FROM Transaction WHERE appName = 'My Web App' SINCE 30 minutes ago"
      },
      "headers": {
        "X-Query-Key": "YOUR_INSIGHTS_KEY"
      }
    },
    "infrastructure_hosts": {
      "url": "https://infra-api.newrelic.com/v2/hosts",
      "description": "Get infrastructure hosts data",
      "headers": {
        "X-Api-Key": "YOUR_API_KEY"
      }
    },
    "alerts_violations": {
      "url": "https://api.newrelic.com/v2/alerts_violations.json",
      "description": "Get alert violations",
      "parameters": {
        "only_open": "true"
      }
    }
  }
